{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "ES2020",
    "module": "ESNext",
    "strict": false,
    "moduleResolution": "Bundler",
    "lib": ["ESNext", "DOM", "DOM.Iterable", "WebWorker"],
    "types": [
      "vite/client"
    ],
    "baseUrl": "./docs",
    "paths": {
      "@vueuse/*": ["./analysis/vueuse/code/*"]
    }
  },
  "include": [
    "docs/**/*.ts",
    "docs/.vitepress/**/*.ts"
  ],
}